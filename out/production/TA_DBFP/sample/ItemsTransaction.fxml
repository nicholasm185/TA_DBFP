                <TableColumn fx:id="productIDCartCol" prefWidth="98.0" text="Product ID" />
                <TableColumn fx:id="productNameCartCol" prefWidth="147.0" text="Product Name" />
                <TableColumn fx:id="priceCartCol" prefWidth="93.0" text="Price @" />
