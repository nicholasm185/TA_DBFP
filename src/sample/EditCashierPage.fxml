<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="440.0" prefWidth="342.0" stylesheets="@style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.EditCashierController">
   <children>
      <Label layoutX="101.0" layoutY="29.0" text="Edit Cashier">
         <font>
            <Font name="Arial Black" size="21.0" />
         </font>
      </Label>
      <ComboBox fx:id="adminCombo" layoutX="96.0" layoutY="335.0" prefWidth="150.0" />
      <Label layoutX="152.0" layoutY="78.0" text="Name">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="97.0" layoutY="102.0" />
      <Label layoutX="126.0" layoutY="137.0" text="Old Password">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <PasswordField fx:id="oldPass" layoutX="97.0" layoutY="161.0" />
      <Label layoutX="122.0" layoutY="197.0" text="New Password">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <PasswordField fx:id="passField" layoutX="97.0" layoutY="221.0" />
      <PasswordField fx:id="confirmPass" layoutX="97.0" layoutY="275.0" />
      <Label layoutX="112.0" layoutY="254.0" text="Confirm password">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Label layoutX="128.0" layoutY="310.0" text="Admin Status">
         <font>
            <Font name="Arial Black" size="12.0" />
         </font>
      </Label>
      <Button layoutX="96.0" layoutY="387.0" mnemonicParsing="false" onAction="#updateCashierButtonClicked" prefHeight="25.0" prefWidth="150.0" text="Update Cashier" />
   </children>
</AnchorPane>
